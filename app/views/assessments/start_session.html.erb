<% @patient_exercise_ids.each do |patient_exercise| %>
  <h3> <%= patient_exercise.exercise.name %> </h3>
  <p>
    <%= link_to 'Sucesso', success_patient_exercise_path(patient_exercise), data: { "turbo-method": :patch } %>
    <%= label_tag patient_exercise.success %>
  </p>
  <p>
    <%= link_to 'Falha', failed_patient_exercise_path(patient_exercise), data: { "turbo-method": :patch } %>
    <%= label_tag patient_exercise.failed %>
  </p>
  <p>
    <%= link_to 'Com ajuda', help_patient_exercise_path(patient_exercise), data: { "turbo-method": :patch } %>
    <%= label_tag patient_exercise.help %>
  </p>
<% end %>
