<h1>Cadastrar paciente</h1>

<%= link_to 'Backoffice', backoffice_path %>
<%= form_with(model: @patient, local: true) do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="actions">
    <%= form.submit 'Create Patient' %>
  </div>
<% end %>
