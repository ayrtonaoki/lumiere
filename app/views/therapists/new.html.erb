<h1>Cadastrar Terapeuta</h1>

<div>
  <%= flash[:message] %>
</div>

<%= link_to 'Voltar', backoffice_path %>
<%= form_with(model: @therapist, local: true) do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="actions">
    <%= form.submit 'Criar Terapeuta' %>
  </div>
<% end %>
